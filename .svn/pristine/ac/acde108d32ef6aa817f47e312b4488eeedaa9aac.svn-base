			<span data-toggle="modal" data-target="#aliPayModal">支付宝扫码</span>
			<!--<span data-toggle="modal" data-target="#cashWithdrawModal">充值查询</span>-->
			<!--<span data-toggle="modal" data-target="#cashWithdrawModal">数据更新</span>-->
			<!--<span data-toggle="modal" data-target="#cashWithdrawModal">系统日结</span>-->
