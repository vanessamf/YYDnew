.pad-content .pad-bg{
	position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
}
.pad-content .overlay {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(255,255,255,0.35);
    z-index: -1;
    }
.pad-content .left-menu{
	width: 200px;
	overflow-y: scroll;
}

.pad-content .left-menu>ul>li.left-item{
	
	color:#fff;
	/*height: 70px;*/
	line-height: 70px;
	border-bottom: 1px solid #cad9de;
	text-align: center;
}
.pad-content .left-menu ul li:last-child{
	border:none;
}
.pad-content .left-menu>ul>li>a{
	background: rgba(112,163,172,.8);
}
.pad-content .left-menu>ul>li.active>a{
	background: #85bfca;
}
.pad-content .left-menu>ul>li>a:focus,
