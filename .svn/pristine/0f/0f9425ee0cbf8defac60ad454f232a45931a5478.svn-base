	<script type="text/javascript">
		$(function() {
			$(".cash-btn").click(function() {
				window.location.href = "payment.html";
			})
			var menuContentHeight = $(".menu-wrapper").height() - $(".menu-wrapper .room-tab").height() - $(".menu-wrapper .menu-other").height();
			$(".menu-wrapper .menu-content").height(menuContentHeight);
		})
		scrollWidth1(0, "order-title", "order-content");
		scrollWidth1(0, "room-tab", "tab-content>ul");
		// 打开键盘
		$(document).on('click', '.btn-keyboard', function() {
			$(".keyboard-com").toggleClass('on');
		})
		// 退出键盘
		$(".keys").on('click', 'li', function() {
			var $this = $(this);
			var $txt = $this.text()
			if($txt == '退出') {
				$(".keyboard-com").removeClass('on');
			}
		})
	</script>
	<script type="text/javascript">
		//赠送原因切换
		$(".give-away-container .reason-btn").on("click", function() {
			if(!$(this).hasClass("active")) {
				$(this).siblings(".reason-btn").removeClass("active");
				$(this).siblings(".reason-btn").children("i").remove();
				$(this).addClass("active");
				var html = '<i class="iconfont icon-gou"></i>';
				$(this).append(html);
			}
		})
		//套餐切换
		$(".set-meal-container .meal-item").on("click", function() {
			if(!$(this).hasClass("active")) {
				$(this).siblings(".meal-item").removeClass("active");
				$(this).siblings(".meal-item").children("i").remove();
				$(this).addClass("active");
				var html = '<i class="iconfont icon-gou"></i>';
				$(this).append(html);
			}
		})
	</script>

