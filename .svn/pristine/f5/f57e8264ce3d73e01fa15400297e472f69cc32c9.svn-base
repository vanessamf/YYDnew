/*-----------------------------------登录------------------------------------------------*/
.login-wrapper {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	background: #fff;
	height: 100%;
	padding: 25px;
}

.login-head {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.login-head .logo img {
	height: 40px;
}

.login-head .operating-btn i {
	padding: 20px;
	font-size: 20px;
}

.login-container {
	display: flex;
	justify-content: center;
	align-items: center;
	flex: 1;
	height:100%;
}

.login-content {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 80%;
	height: 60%;
	min-height: 300px;
}

.login-left {
	width: 60%;
	height: 100%;
	padding-right: 14px;
}

.login-left .login-input {
	display: flex;
	justify-content: space-between;
}

.login-left .login-input input {
	/*width: 227px;*/
	width: 48%;
	height: 70px;
	color: white;
	font-size: 30px;
	border-radius: 10px;
	background: #fdd072;
	border: none;
	outline: none;
	padding: 0 10px !important;
}

.login-left .login-input input::-webkit-input-placeholder {
	color: #fff;
}

.login-keyboard {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	align-content: space-between;
	/*width: 477px;*/
	width: 100%;
	height: calc(100% - 84px);
	margin-top: 14px;
}

.login-keyboard span {
	display: flex;
	justify-content: center;
	align-items: center;
	text-align: center;
	/*box-sizing: border-box;*/
	color: #59ccde;
	font-size: 27px;
	float: left;
	/*width: 152.333px;*/
	width: 31.5%;
	/*height: 63.75px;*/
	height: 22%;
	/*line-height: 63.75px;*/
	border: solid #59ccde;
	border-radius: 10px;
}

.login-keyboard span:active {
	background: #e6e6e6;
}

.login-keyboard span .icon-shanchu {
	font-size: 36px;
	font-weight: 700;
}

.login-right {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	/*align-items: center;*/
	width: 40%;
	height: 100%;
}

.login-right .btn-login {
	/*width: 363px;*/
	width: 100%;
	/*height: 171px;*/
	height: 48%;
	font-size: 40px;
	background: #59ccde;
}
.login-right .btn-login:hover,
.login-right .btn-login:active {
	background: #45d6d1;
	/*background:#42adbe;*/
}

.login-right .btn-login span {
	display: block;
	color: #fff;
	font-size: 20px;
}

.login-right .btn-login span:first-child {
	font-size: 40px;
}

.login-foot {
	display: flex;
	justify-content: space-between;
	align-items: center;
	width: 80%;
	margin: 0 auto;
	text-align: center;
}

.login-foot .line {
	display: inline-block;
	flex: 1;
	border-top: 2px solid #59ccde;
}

.login-foot .txt {
	padding: 0 5px;
	font-size: 16px;
}

.version-info {
	margin-top: 16px;
	text-align: right;
	font-size: 16px;
}
/*-------------------------登录END--------------------------------------------*/

