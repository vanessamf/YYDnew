.member-wrapper {
	position: relative;
	/*padding: 7px 0 0 0;*/
	height: calc(100% - 85px);
	font-size: 16px;
}


/*左边*/

.member-left {
	height: 100%;
	background: #7aa9b1;
	color: #fff;
	overflow-y: auto;
}

.member-left .left-menu li {
	display: flex;
	justify-content: center;
	height: 70px;
	line-height: 70px;
	padding: 0 20px;
	overflow: hidden;
	border-bottom:1px solid #7bbfcc;
	/*text-align: center;*/
	/*white-space: nowrap;
	text-overflow: ellipsis;*/
}

.member-left .left-menu li.active,
.member-left .left-menu li:active {
	/*background: #7eb3bb;*/
	background: #7bbfcc;
}

.member-left .left-menu li i {
	margin-right: 10px;
	font-size: 22px;
}


/*右边*/

.member-right {
	height: 100%;
	padding: 15px;
}

.member-right .right-content {
	height: 100%;
	background: #fff;
	box-shadow: 0px 1px 6px 2px #c9c6c6;
}
/*注册*/
.registration-content {
    height: 100%;
    padding: 20px;
}
.registration-content .form-container .form-group {
	height:35px;
	line-height: 35px;
}
.registration-content .form-container .form-group .control-label{
	padding:0;
	font-weight: 400;
	text-align: left;
}
.registration-content .form-container .form-group .form-control{
	border-radius:0;
}
.registration-content .form-container .form-group .radio-inline{
	padding-top:0;
	padding-right:20px;
}
.registration-content .form-container .form-group .radio-inline input[type=radio]{
	top:8px;
}
.registration-content .form-container .form-group .calendar-wrapper .date{
	position: relative;
	}
.registration-content .form-container .form-group.calendar-wrapper .date .add-on{
	position: absolute;
    top: 0;
    right: 0;
    z-index: 10;
}
.registration-content .form-container .form-group.calendar-wrapper .date .add-on i {
    padding: 0 5px;
    /* line-height: .4rem; */
    line-height: 35px;
    font-size: 16px;
    font-weight: 700;
    color: #778091;
    float: left;
    font-style: normal;
}
.registration-content .form-container .form-group .sel2-control{
	width:100%;
}
.registration-content .form-container .form-group .required{
	padding-left:5px;
	color:#e32a0c;
}

/*键盘*/
.registration-keyboard {
	/*width: 100%;*/
	/*padding: 0 10px;*/
	height: -webkit-fit-content;
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	align-items: flex-end;
	background: #fff;
	z-index: 9999;
}

.registration-keyboard .key {
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
	width: 31%;
	height: 60px;
	margin-bottom: 10px;
	background: #eeeeee;
	/*border: 1px solid #59ccde;*/
	border-radius: 6px;
}

.registration-keyboard .key:active {
	color: #fff;
	/*background: #59ccde;*/
	background: #0ac7c1;
}
