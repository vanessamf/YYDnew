<script type="text/javascript">
	//销售类型切换
	$(".saleType-container ul li").on("click", function() {
		if(!$(this).hasClass("active")) {
			$(this).siblings("li").removeClass("active");
			$(this).siblings("li").children("i").remove();
			$(this).addClass("active");
			var html = '<i class="iconfont icon-gou"></i>';
			$(this).append(html);
		}
	})
	//赠送原因切换
	$(".give-away-container .reason-btn").on("click", function() {
		if(!$(this).hasClass("active")) {
			$(this).siblings(".reason-btn").removeClass("active");
			$(this).siblings(".reason-btn").children("i").remove();
			$(this).addClass("active");
			var html = '<i class="iconfont icon-gou"></i>';
			$(this).append(html);
		}
	})
	//套餐切换
	$(".set-meal-container .meal-item").on("click", function() {
		if(!$(this).hasClass("active")) {
			$(this).siblings(".meal-item").removeClass("active");
			$(this).siblings(".meal-item").children("i").remove();
			$(this).addClass("active");
			var html = '<i class="iconfont icon-gou"></i>';
			$(this).append(html);
		}
	})
	//反结算原因切换
	$(".overring-container .reason-btn").on("click", function() {
		if(!$(this).hasClass("active")) {
			$(this).siblings(".reason-btn").removeClass("active");
			$(this).siblings(".reason-btn").children("i").remove();
			$(this).addClass("active");
			var html = '<i class="iconfont icon-gou"></i>';
			$(this).append(html);
		}
	})
	// 会员卡付款打开键盘
	$(document).on('click', '.vip-key-input', function() {
		$(".vip-keyboard").toggleClass('on');
	})
	// 会员卡付款退出键盘
	$(".keys").on('click', 'li', function() {
		var $this = $(this);
		var $txt = $this.text()
		if($txt == '退出') {
			$(".keyboard-com").removeClass('on');
		}
	})
	// 打开大键盘
	$(document).on('click', '.btn-keyboard', function() {
		$(".keyboard-com").toggleClass('on');
	})
	// 退出大键盘
	$(".keys").on('click', 'li', function() {
		var $this = $(this);
		var $txt = $this.text()
		if($txt == '退出') {
			$(".keyboard-com").removeClass('on');
		}
	})
	//数据更新动画
	$(".circleChart").circleChart({
            size: 80,
            value: 100,
            text: 0,
            textCenter:false,
            textSize:'20px',
            color:'rgba(255,255,255,0.8)',
            backgroundColor:'rgba(255,255,255,.2)',
            speed:3000,
            animation:'easeInOutSine',
//          autoCss:false,
            onDraw: function(el, circle) {
                circle.text(Math.round(circle.value) + "%");
            }
        });
</script>