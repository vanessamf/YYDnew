.icon-arrow-left:before {
	content: "\e091";
}

.icon-arrow-right:before {
	content: "\e092";
}


/*flex*/

.flex {
	display: box;
	/* OLD - Android 4.4- */
	display: -webkit-box;
	/* OLD - iOS 6-, Safari 3.1-6 */
	display: -moz-box;
	/* OLD - Firefox 19- (buggy but mostly works) */
	display: -ms-flexbox;
	/* TWEENER - IE 10 */
	display: -webkit-flex;
	/* NEW - Chrome */
	display: flex;
	/* NEW, Spec - Opera 12.1, Firefox 20+ */
}

.flex-around {
	-moz-box-pack: justify;
	-webkit-box-pack: justify;
	-ms-flex-pack: justify;
	-webkit-justify-content: space-around;
	justify-content: space-around;
}

.flex-between {
	-moz-box-pack: justify;
	-webkit-box-pack: justify;
	-ms-flex-pack: justify;
	-webkit-justify-content: space-between;
	justify-content: space-between;
}


/* 父元素-竖直居中（主轴是横向才生效） */

.flex-vc {
	-webkit-box-align: center;
	-webkit-align-items: center;
	-moz-align-items: center;
	-ms-align-items: center;
	-o-align-items: center;
	align-items: center;
}


/*表格*/

.table {
	display: table;
	width: 100%;
	margin-bottom: 0;
	border-collapse: collapse;
}

.table-tr {
	display: table-row;
	height: 35px;
	width: 100%;
}

.table-thead {
	display: table-header-group;
}

.table-tbody {
	display: table-row-group;
}

.table-th {
	display: table-cell;
	/*font-weight: bold;*/
	height: 35px;
	border: 1px solid gray;
	text-align: center;
	vertical-align: middle;
	background-color: #eee;
	border: none;
}

.table-td {
	display: table-cell;
	height: 35px;
	border: 1px solid gray;
	text-align: center;
	vertical-align: middle;
	border: none;
}


/*日历*/
.datetimepicker th.switch {
    width:200px;
}
.datetimepicker td, .datetimepicker th {
    width: 40px;
    height: 40px;
}
.datetimepicker .datetimepicker-hours span {
    height: 40px;
    line-height: 40px;
}
.datetimepicker .datetimepicker-minutes span {
    height: 40px;
    line-height: 40px;
}
/*滚动条*/

::-webkit-scrollbar {
	width: 0;
	height: 0;
}


/*---------------------------------------------头部-----------------------------------------------*/

.header {
	padding: 0 20px;
	height: 50px;
	line-height: 50px;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 16px;
	color: #fff;
	background: linear-gradient(to top, #29a8b9, #80e9fc);
	background: -webkit-linear-gradient(to top, #29a8b9, #80e9fc);
}

.header .logo {
	padding: 0;
}

.header img {
	wdith: 130px;
	height: 33px;
}

.header .back {
	display: flex;
}

.header .back:focus {
	color: #fff;
}

.header .title {
	flex:1;
	text-align: center;
}

.header .back i {
	display: inline-block;
	margin-right: 5px;
	font-size: 20px;
}

.status-wrapper {
	flex: 1;
}

.status-wrapper span {
	margin-left: 25px;
}

.bootstrap-switch {
	background: #fff;
	border-radius: 20px;
	height: 28px;
}

.bootstrap-switch .bootstrap-switch-handle-off,
.bootstrap-switch .bootstrap-switch-handle-on,
.bootstrap-switch .bootstrap-switch-label {
	padding: 3px 5px;
	border-radius: 20px;
}

.bootstrap-switch.bootstrap-switch-inverse.bootstrap-switch-on .bootstrap-switch-label,
.bootstrap-switch.bootstrap-switch-off .bootstrap-switch-label {
	border-radius: 20px;
}

.header .nav {
	padding: 0;
	height: 42px;
	line-height: 42px;
	font-size: 0;
	text-align: right;
}

.header .nav .nav-item {
	padding: 0 20px;
	margin-left: 6px;
	display: inline-block;
	font-size: 16px;
	background: #31c9e0;
	border-radius: 6px;
	cursor: pointer;
}

.btn-exit {
	padding: 0;
	/*margin-left: 6px;*/
	height: 100%;
	/*line-height: 36px;*/
	/*background: #31c9e0;
	border-radius: 6px;*/
	text-align: right;
	padding: 0 20px;
}

.btn-exit i {
	font-size: 28px;
}


/*-----------------------------------------------头部END-----------------------------------------------------*/


/*-------------------------------------------------底部--------------------------------------------------------------*/

.footer {
	position: fixed;
	left: 0;
	bottom: 0;
	/*padding:0 20px;*/
	width: 100%;
	height: 35px;
	display: flex;
	justify-content: space-between;
	align-items: center;
	color: #fff;
	background: #799195;
	font-size: 14px;
}

.footer div {
	padding: 0 15px;
	display: flex;
	justify-content: space-between;
	align-items: center;
	overflow: hidden;
	white-space: nowrap;
	/*text-overflow: ellipsis;*/
}

.footer div span {
	margin-right: 20px;
}

.footer div span:last-child {
	margin-right: 0;
}

.footer div span i {
	display: inline-block;
	font-size: 16px;
}


/*--------------------------------------------------------------底部END--------------------------------------------------------------*/


