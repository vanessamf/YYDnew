}

.btn-wrapper {
	height: 50%;
	display: flex;
	flex-wrap: wrap;
	align-items: flex-end;
}

.btn-wrapper .menu {
	width: 100%;
	display: flex;
	justify-content: space-between;
	height: 22%;
}

.btn-wrapper .menu .key {
	width: 48%;
