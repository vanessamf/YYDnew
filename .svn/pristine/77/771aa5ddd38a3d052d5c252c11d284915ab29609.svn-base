.keyboard-container .key {
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
}

.keyboard-container .keyboard .key {
	width: 31%;
	height: 22%;
	/*margin-bottom: 10px;*/
	background: #fff;
	/*border: 1px solid #59ccde;*/
	border-radius: 0.08rem;
	/*border:1px solid rgba(128, 119, 119, 0.21);*/
	border-bottom-color: rgba(70, 72, 71, 0.34);
	/*text-shadow: 0 1px 0 rgba(0,0,0,0.15);*/
