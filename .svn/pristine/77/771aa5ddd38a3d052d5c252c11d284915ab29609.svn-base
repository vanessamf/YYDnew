.bill-list-container li.checkbox label {
	width: 100%;
	padding-left: 0.5rem;
}

.bill-list-container li.checkbox label:before {
	content: '';
	position: absolute;
	left: 0.15rem;
	top: 0.1rem;
	width: 0.15rem;
	height: 0.15rem;
	border: 1px solid #ddd;
	border-radius: 50%;
	transition: all 0.3s ease;
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
}

.bill-list-container li.checkbox label.checked:before {
	border: none;
	background: #fea018;
}

.bill-list-container li.checkbox label.checked.grey-checked:before {
	background: #aaa;
}

.bill-list-container li.checkbox label.checked:after {
	content: '';
	position: absolute;
	left: 0.2rem;
	top: 0.12rem;
	width: 0.06rem;
	height: 0.1rem;
	border-right: 0.02rem solid #fff;
	border-bottom: 0.02rem solid #fff;
	transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	transition: all 0.3s ease;
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
}

.bill-list-container li.checkbox input[type=checkbox] {
	margin-left: -0.3rem;
	margin-top: 0.13rem;
	opacity: 0;
}


