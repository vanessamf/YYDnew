	/*border-right: 1px solid #d2d2d2;*/
}

.separateBills-container .bills-column:last-child {
	/*border: none;*/
}

.bills-column .column-title {
	display: flex;
	justify-content: space-between;
	/*padding-left:15px;*/
	line-height: 0.4rem;
	height: 0.4rem;
	background: #eee;
}

.separateBills-container .checkbox {
	position: relative;
	margin: 0;
}


/*.bills-column .column-title .checkbox {
	
}*/

.separateBills-container .checkbox label {
	width: 100%;
	padding-left: 0.5rem;
}


/*.bills-column .column-title .checkbox label {
	width: 100%;
	padding-left: 50px;
}
.bills-column .column-content .dish-list label {
	width: 100%;
	padding-left: 50px;
}*/

.separateBills-container .checkbox label:before {
	content: '';
	position: absolute;
	left: 0.15rem;
	top: 0.1rem;
	width: 0.15rem;
	height: 0.15rem;
	border: 1px solid #ddd;
	border-radius: 50%;
	transition: all 0.3s ease;
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
}

.separateBills-container .column-title .checkbox label:before {
	top: 0.125rem;
}

.separateBills-container .checkbox label.checked:before {
	border: none;
	background: #fea018;
}

.separateBills-container .checkbox label.checked:after {
	content: '';
	position: absolute;
	left: 0.2rem;
	top: 0.15rem;
	width: 0.06rem;
	height: 0.1rem;
	border-right: 0.02rem solid #fff;
	border-bottom: 0.02rem solid #fff;
	transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	transition: all 0.3s ease;
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
