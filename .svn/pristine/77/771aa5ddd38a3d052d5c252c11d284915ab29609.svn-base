	/*padding: 10px 25px;*/
}

.desk-wrapper .desk-tips div {
	display: flex;
	/*justify-content: space-between;*/
	align-items: center;
	overflow: hidden;
	white-space: nowrap;
}

.desk-wrapper .desk-tips span {
	margin-right: 0.2rem;
	display: flex;
	align-items: flex-end;
}

.desk-wrapper .desk-tips span.text-using {
	color: #53d8d3;
}

.desk-wrapper .desk-tips span.text-usedTime {
	color: #ed5258;
}

.desk-wrapper .desk-tips span.text-clear {
	color: #9d9d9d;
}

.desk-wrapper .desk-tips span.text-num {
	color: #59ccde;
}

.desk-wrapper .desk-tips span i {
	line-height: 0.2rem;
	/*margin-right: 10px;*/
	font-size: 0.3rem;
}

.desk-wrapper .desk-tips span i.icon-icon_shangyipian,
.desk-wrapper .desk-tips span i.icon-icon_xiayipian {
	font-size: 0.3rem;
	color: #a0a0a0;
}


/*tableList右边*/

.command-wrapper {
	/*width: 35%;*/
	height: 100%;
	padding: 0.1rem 0.1rem;
	background: #b6dee0;
