/*-----------------------------------登录------------------------------------------------*/

.login-wrapper {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	background: #fff;
	height: 100%;
	padding: 0.25rem;
}

.login-head {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.login-head .logo img {
	height: 0.4rem;
}

.login-head .operating-btn i {
	padding: 0.2rem;
	font-size: 0.2rem;
}

.login-container {
	display: flex;
	justify-content: center;
	align-items: center;
	flex: 1;
	height: 100%;
}

.login-content {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 80%;
	height: 60%;
	min-height: 3rem;
}

.login-left {
	width: 60%;
	height: 100%;
	padding-right: 0.14rem;
}

.login-left .login-input {
	display: flex;
	justify-content: space-between;
}

.login-left .login-input input {
	/*width: 227px;*/
	width: 48%;
	height: 0.7rem;
	color: white;
	font-size: 0.3rem;
	border-radius: 0.1rem;
	background: #fdd072;
	border: none;
	outline: none;
	padding: 0 0.1rem !important;
}

.login-left .login-input input::-webkit-input-placeholder {
	color: #fff;
}

.login-keyboard {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	align-content: space-between;
	/*width: 477px;*/
	width: 100%;
	height: calc(100% - 0.84rem);
	margin-top: 0.14rem;
}

.login-keyboard span {
	display: flex;
	justify-content: center;
	align-items: center;
	text-align: center;
	/*box-sizing: border-box;*/
	color: #59ccde;
	font-size: 0.27rem;
	float: left;
	/*width: 152.333px;*/
	width: 31.5%;
	/*height: 63.75px;*/
	height: 22%;
	/*line-height: 63.75px;*/
	border: solid #59ccde;
	border-radius: 0.1rem;
}

.login-keyboard span:active {
	background: #e6e6e6;
}

.login-keyboard span .icon-shanchu {
	font-size: 0.36rem;
	font-weight: 700;
}

.login-right {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	/*align-items: center;*/
	width: 40%;
	height: 100%;
}

.login-right .btn-login {
	/*width: 363px;*/
	width: 100%;
	/*height: 171px;*/
	height: 48%;
	font-size: 0.4rem;
	background: #59ccde;
	border: 1px solid rgba(128, 119, 119, 0.21);
	border-bottom-color: rgba(70, 72, 71, 0.34);
	text-shadow: 0 2px 0 rgba(0, 0, 0, 0.15);
	box-shadow: 0 1px 0 rgba(255, 255, 255, 0.34) inset, 0 2px 0 -1px rgba(0, 0, 0, 0.13), 0 3px 0 -1px rgba(0, 0, 0, 0.08), 0 3px 13px -1px rgba(0, 0, 0, 0.21);
}

.login-right .btn-login:hover,
.login-right .btn-login:active {
	background: #53d8d3;
	/*background:#42adbe;*/
	border: 1px solid rgba(128, 119, 119, 0.21);
	border-bottom-color: rgba(70, 72, 71, 0.34);
	text-shadow: 0 1px 0 rgba(0, 0, 0, 0.15);
	box-shadow: 0 1px 0 rgba(255, 255, 255, 0.34), 0 2px 0 -1px rgba(0, 0, 0, 0.13) inset, 0 3px 0 -1px rgba(0, 0, 0, 0.08) inset, 0 3px 13px -1px rgba(0, 0, 0, 0.21) inset;
}

.login-right .btn-login span {
	display: block;
	color: #fff;
	font-size: 0.2rem;
}

.login-right .btn-login span:first-child {
	font-size: 0.4rem;
}

.login-foot {
	display: flex;
	justify-content: space-between;
	align-items: center;
	width: 80%;
	margin: 0 auto;
	text-align: center;
}

.login-foot .line {
	display: inline-block;
	flex: 1;
	border-top: 0.02rem solid #59ccde;
}

.login-foot .txt {
	padding: 0 0.05rem;
	font-size: 0.16rem;
}

.version-info {
	margin-top: 0.16rem;
	text-align: right;
	font-size: 0.16rem;
}


/*-------------------------登录END--------------------------------------------*/


/*-------------------------------------------------主体--------------------------------------------------------------*/

.main-wrapper {
	position: relative;
	/*padding: 7px 0 12px 0;*/
	padding: 0.07rem 0 0 0;
	height: calc(100% - 0.85rem);
	font-size: 0.16rem;
}


/*tableList左边*/

.desk-wrapper {
	/*width: 65%;*/
	padding-bottom: 0.12rem;
	height: 100%;
	position: relative;
}

.desk-wrapper .room-tab ul {
	display: flex;
	width: 100%;
	height: 0.76rem;
	line-height: 0.76rem;
	margin-bottom: 0.05rem;
	overflow: hidden;
}

.desk-wrapper .room-tab .tab-nav {
	/*flex: 1 0 auto;*/
	width: calc(20% - 0.05rem);
	height: 100%;
	margin-left: 0.05rem;
	background: #59ccde;
	/*float: left;*/
	text-align: center;
	color: #fff;
}

@media (min-width: 1200px) {
	.desk-wrapper .room-tab .tab-nav {
		width: calc(14.28% - 0.05rem);
	}
}

@media (min-width: 1600px) {
	.desk-wrapper .room-tab .tab-nav {
		width: calc(12.5% - 0.05rem);
	}
}

.desk-wrapper .room-tab .tab-nav a {
	height: 0.76rem;
	line-height: 0.76rem;
	padding: 0;
	border-radius: 0px;
}

.desk-wrapper .room-tab .tab-nav a.arrow {
	float: left;
	width: 50%;
}

.desk-wrapper .room-tab .nav-pills>li a:hover {
	color: #59ccde;
	background-color: #fff;
}

.desk-wrapper .room-tab .nav-pills>li.active>a,
.desk-wrapper .room-tab .nav-pills>li.active>a:focus,
.desk-wrapper .room-tab .nav-pills>li.active>a:hover {
	color: #59ccde;
	background-color: #fff;
	border: 1px solid #59ccde;
}

.desk-wrapper .tab-content {
	height: calc(100% - 1.21rem);
	overflow-y: auto;
}

.desk-wrapper .tab-content::-webkit-scrollbar {
	width: 0px;
	height: 0px;
}

.desk-wrapper .desks {
	/*width: 100%;*/
	/*height: calc(100% - 81px);*/
}

.desk-wrapper .desks .desk-item {
	width: calc(20% - 0.05rem);
	height: 0.99rem;
	/*line-height: 99px;*/
	margin-left: 0.05rem;
	margin-bottom: 0.05rem;
	text-align: center;
	background: #fff;
	border: 1px solid #c7c7c7;
	float: left;
}

@media (min-width: 1200px) {
	.desk-wrapper .desks .desk-item {
		width: calc(14.28% - 0.05rem);
	}
}

@media (min-width: 1600px) {
	.desk-wrapper .desks .desk-item {
		width: calc(12.5% - 0.05rem);
	}
}

.desk-wrapper .desks .desk-item {
	position: relative;
	padding: 0.35rem 0;
	overflow: hidden;
}

.desk-wrapper .desks .desk-item.guest {
	padding: 0.05rem 0;
	color: #fff;
	background: #53d8d3;
}

.desk-wrapper .desks .desk-item.guest h3 {
	font-size: 0.18rem !important;
}

.desk-wrapper .desks .desk-item.guest p {
	font-size: 0.14rem;
}

.desk-wrapper .desks .desk-item .badge {
	position: absolute;
	top: 0;
	left: 0;
	padding: 0;
	background: none;
	border-bottom: 0.4rem solid transparent;
	border-left: 0.4rem solid #f0ad4e;
	border-radius: 0;
}

