	height: 0.5rem;
	line-height: 0.5rem;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 0.16rem;
	color: #fff;
	background: linear-gradient(to top, #29a8b9, #80e9fc);
	background: -webkit-linear-gradient(to top, #29a8b9, #80e9fc);
}

.header .logo {
	padding: 0;
	
}
.header img {
	wdith:1.3rem;
	height:0.33rem;
}
