	}
	
	.menu-wrapper .room-tab .tab-nav {
		/*flex: 1 0 auto;*/
		width: calc(20% - 5px);
		height: 100%;
		margin-left: 5px;
		margin-bottom: 5px;
		background: #59ccde;
		/*float: left;*/
