.member-wrapper {
	position: relative;
	/*padding: 7px 0 0 0;*/
	height: calc(100% - 85px);
	font-size: 16px;
}


/*左边*/

.member-left {
	height: 100%;
	background: #7aa9b1;
	color: #fff;
	overflow-y: auto;
}

.member-left .left-menu li {
	display: flex;
	justify-content: center;
	height: 70px;
	line-height: 70px;
	padding: 0 20px;
	overflow: hidden;
	border-bottom:1px solid #7bbfcc;
	/*text-align: center;*/
	/*white-space: nowrap;
	text-overflow: ellipsis;*/
}

.member-left .left-menu li.active,
.member-left .left-menu li:active {
	/*background: #7eb3bb;*/
	background: #7bbfcc;
}

.member-left .left-menu li i {
	margin-right: 10px;
	font-size: 22px;
}


/*右边*/

.member-right {
	height: 100%;
	padding: 15px;
}

.member-right .right-content {
	height: 100%;
	background: #fff;
	box-shadow: 0px 1px 6px 2px #c9c6c6;
}
/*注册*/
.registration-content {
    height: 100%;
    padding: 20px;
}
.registration-content  .form-group {
	height:35px;
	line-height: 35px;
	margin-left:0;
	margin-right:0;
}
.registration-content  .form-group .control-label{
	padding:0;
	font-weight: 400;
	text-align: left;
}
.registration-content  .form-group .form-control{
	border-radius:0;
}
.registration-content .form-group .radio-inline{
	padding-top:0;
	padding-right:20px;
}
.registration-content .form-group .radio-inline input[type=radio]{
	top:8px;
}
.registration-content  .form-group .calendar-wrapper .date{
	position: relative;
	}
.registration-content .form-container .form-group.calendar-wrapper .date .add-on{
	position: absolute;
    top: 0;
    right: 0;
    z-index: 10;
}
.registration-content .form-container .form-group.calendar-wrapper .date .add-on i {
    padding: 0 5px;
    /* line-height: .4rem; */
    line-height: 35px;
    font-size: 16px;
    font-weight: 700;
    color: #778091;
    float: left;
    font-style: normal;
}
.registration-content .form-container .form-group .sel2-control{
	width:100%;
}

.registration-content .form-container .form-group  .select2-container .select2-choice{
	height:35px;
	line-height:35px;
}

.registration-content .form-container .form-group .required{
	padding-left:5px;
	color:#e32a0c;
}

/*键盘*/
.registration-keyboard {
	/*width: 100%;*/
	/*padding: 0 10px;*/
	height: -webkit-fit-content;
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	align-items: flex-end;
	background: #fff;
	z-index: 9999;
}

.registration-keyboard .key {
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
	width: 31%;
	height: 60px;
	margin-bottom: 10px;
	background: #eeeeee;
	/*border: 1px solid #59ccde;*/
	border-radius: 6px;
}

.registration-keyboard .key:active {
	color: #fff;
	/*background: #59ccde;*/
	background: #53d8d3;
}
.confirm-apply-account {
    height: 60px;
    line-height: 60px;
    /*-webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    border-radius: 10px;*/
    font-size: 18px;
    cursor: pointer;
    width: 99%;
    text-align: center;
    letter-spacing: 3px;
    color: #fff;
    background: #53d8d3;
     /*background: #cfe2e6;*/
    /*-webkit-box-shadow: inset 0 0 10px #0ac7c1;
    -moz-box-shadow: inset 0 0 10px #0ac7c1;
    box-shadow: inset 0 0 10px #0ac7c1;*/
}
.confirm-apply-account:active {
    /*position: relative;
    top: 1px;
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;*/
}

/*充值*/
.registration-content.recharge-content  .form-horizontal{
	/*display: flex;
    align-items: center;*/
}
.registration-content.recharge-content  .form-horizontal>div:nth-child(2){
	margin-top:90px;
}
.registration-content.recharge-content  .form-horizontal>div:nth-child(3){
	margin-top:50px;
}
.registration-content.recharge-content .form-group{
	margin-bottom:10px;
}
.search-group{
	position: relative;
}
.search-group span{
	position: absolute;
	top:0;
	right:0;
	padding:0 5px;
	color:#53d8d3;
}

.registration-content .payType{
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.registration-content .payType span{
	display: block;
	width:31%;
	line-height:55px;
	text-align: center;
	background: #d5f1f4;
	border-radius: 5px;
}
.registration-content .payType span.active,
.registration-content .payType span:active{
	color:#fff;
	background:#53d8d3;
}


/*查询*/
.query-wrapper {
    height: 100%;
    font-size: 16px;
    background: #fff;
}
.query-wrapper .query-header ul {
    /* line-height: 50px; */
    padding: 5px 0px 5px 20px;
}
.query-wrapper .query-header ul>li {
    display: inline-block;
    /* margin-right: 10px; */
    margin: 5px 0;
    height: 35px;
    line-height: 35px;
}
.query-wrapper .query-header ul>li .control-label {
    float: left;
    font-weight: 400;
    /* max-width: 75px; */
    max-width: 100px;
    margin-bottom: 0;
    height: 35px;
    line-height: 35px;
    padding: 0;
    text-align: center;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.query-wrapper .query-header ul>li .calendar-panel {
    display: inline-block;
    margin: 0;
    height: 35px;
    line-height: 35px;
}
.query-wrapper .query-header ul>li .calendar-panel .date {
	height: 35px;
	position: relative;
	display: inline-block;
	/*width:200px;*/
}

.query-wrapper .query-header ul>li .calendar-panel .date input {
	float: none;
}

.query-wrapper .query-header ul>li .add-on {
	position: absolute;
	top: 0;
	right: 0;
	z-index: 10;
}

.query-wrapper .query-header ul>li i {
	padding: 0 5px;
	/* line-height: .4rem; */
	line-height: 35px;
	font-size: 16px;
	font-weight: 700;
	color: #778091;
	float: left;
	font-style: normal;
}

.query-wrapper .query-header ul>li .calendar-panel>span {
	display: inline-block;
	/*vertical-align: middle;
	line-height: 35px;*/
	/*vertical-align: middle;*/
	/*width:90px;*/
	height: 100%;
	padding: 0 10px;
	text-align: center;
}
.query-wrapper .query-header ul>li .sm-round-btn {
	display: inline-block;
	position: relative;
	/* border: 1px solid rgba(0,0,0,0.21); */
	float: left;
	vertical-align: middle;
	height: 35px;
	line-height: 35px;
	outline: none;
	padding: 0 10px;
	font-family: inherit;
	font-size: 14px;
	color: #fff;
	text-align: center;
	text-transform: uppercase;
	background: #ff9600;
	border: 1px solid rgba(128, 119, 119, 0.21);
	border-bottom-color: rgba(70, 72, 71, 0.34);
	text-shadow: 0 1px 0 rgba(0, 0, 0, 0.15);
	box-shadow: 0 1px 0 rgba(255, 255, 255, 0.34) inset, 0 2px 0 -1px rgba(0, 0, 0, 0.13), 0 3px 0 -1px rgba(0, 0, 0, 0.08), 0 3px 13px -1px rgba(0, 0, 0, 0.21);
	border-radius: 6px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
}

.query-wrapper .query-header ul>li .sm-round-btn:active {
	position: relative;
	top: 1px;
	border-color: rgba(0, 0, 0, 0.34) rgba(0, 0, 0, 0.21) rgba(0, 0, 0, 0.21);
	box-shadow: 0 1px 0 rgba(255, 255, 255, 0.89), 0 1px rgba(0, 0, 0, 0.05) inset;
}
.query-container {
    height: calc(100% - 60px);
    color: #000;
}
.query-container .member-content {
    height: calc(100% - 60px);
    overflow-y: auto;
}
.query-container  .member-content .table {
	/*height:300px;*/
	/*overflow-y: auto;*/
	table-layout: fixed; 
	word-break: break-all;
	    /*word-wrap: break-word;
    word-break: break-all;*/
}

.query-container  .member-title {
	height: 40px;
	line-height: 40px;
	/*display: flex;
	justify-content: space-between;*/
}

.query-container  .member-list-wrapper{
	height: calc(100% - 40px);
	overflow-x: hidden;
	overflow-y: hidden;
}
.query-container .member-list {
	overflow-y: auto;
}

.query-container  .member-item:nth-child(odd) {
	background: #e7f5f6;
}
.query-container  .member-item span{
	    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.query-container  .member-paging{
	text-align: center;
	line-height: 60px;
}
.query-container  .member-paging i{
	color:#b3b3b3;
	font-size: 26px;
	border-radius:5px;
	border:1px solid #b3b3b3;
}
.query-container  .member-paging i:first-child{
	margin-right:60px;
}